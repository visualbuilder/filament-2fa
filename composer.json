{
    "name": "optimacloud/filament-2fa",
    "description": "Two Factor Auth for filament",
    "keywords": [
        "Optima Cloud",
        "laravel",
        "filament-2fa"
    ],
    "homepage": "https://github.com/optimacloud/filament-2fa",
    "support": {
        "issues": "https://github.com/optimacloud/filament-2fa/issues",
        "source": "https://github.com/optimacloud/filament-2fa"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lee Evans",
            "email": "lee@optimacloud.pro",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.0",
        "kenepa/banner": "^0.0.9",
        "laragear/two-factor": "^2.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-livewire": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Optimacloud\\Filament2fa\\": "src/",
            "Optimacloud\\Filament2fa\\Database\\Factories\\": "database/factories/",
            "Optimacloud\\Filament2fa\\Database\\Migrations\\": "database/migrations/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Optimacloud\\Filament2fa\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Optimacloud\\Filament2fa\\Filament2faServiceProvider"
            ],
            "aliases": {
                "Filament2fa": "Optimacloud\\Filament2fa\\Facades\\Filament2fa"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
